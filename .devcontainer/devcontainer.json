{
  "name": "gyst",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "initializeCommand": "cp -u .env.dev .env && set -a && source .env && set +a",
  "postStartCommand": "curl -fsSL https://deno.land/install.sh | sh && ~/.deno/bin/deno install -g npm:tailwind npm:@tailwindcss/cli",
  "forwardPorts": [
    "5432:5432"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer",
        "vadimcn.vscode-lldb",
        "tamasfe.even-better-toml",
        "fill-labs.dependi",
        "DioxusLabs.dioxus",
        "bradlc.vscode-tailwindcss"
      ]
    }
  },
  "mounts": [
    "source=${localEnv:HOME}/.ssh,target=/home/vscode/.ssh,type=bind,consistency=cached"
  ]
}
